{
  "$schema": "https://json.schemastore.org/tsconfig",
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "paths": {
      "@shared/*": [
        "../../shared/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@stores/*": [
        "./src/stores/*"
      ],
      "@lib/*": [
        "./lib/*"
      ]
    }
  }
}